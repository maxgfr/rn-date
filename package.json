{
  "name": "rn-date",
  "version": "1.0.1",
  "author": "maxgfr",
  "license": "MIT",
  "description": "A zero-dependency library which fixes the date parsing issue in React Native with Android",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/rn-date.git"
  },
  "bugs": {
    "url": "https://github.com/maxgfr/rn-date/issues"
  },
  "homepage": "https://github.com/maxgfr/rn-date#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "rn-date",
    "date",
    "react",
    "react-native",
    "android"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@swc-node/register": "1.5.4",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.21",
    "@swc/jest": "0.2.23",
    "@types/jest": "29.2.3",
    "@types/node": "20.16.11",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "jest": "29.3.1",
    "nodemon": "3.1.7",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "semantic-release": "24.1.2",
    "typescript": "5.6.3"
  }
}
